{
  "manifest_version": 2,
  "name": "Link Saver",
  "version": "1.0",
  "description": "A Chrome extension to save and organize links for future referance",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": ["storage", "tabs"],
  "content_scripts": [
    {
      "js": ["app.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_popup": "index.html"
  }
}
